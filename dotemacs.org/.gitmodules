[submodule "site-lisp/helm"]
	path = site-lisp/helm
	url = https://github.com/emacs-helm/helm.git
	branch = master
[submodule "repos/hdevtools"]
	path = repos/hdevtools
	url = https://github.com/bitc/hdevtools.git
[submodule "site-lisp/flycheck"]
	path = site-lisp/flycheck
	url = https://github.com/lunaryorn/flycheck.git
[submodule "repos/shnippet"]
	path = repos/shnippet
	url = https://github.com/tonyday567/shnippet.git
	branch = master
[submodule "site-lisp/haskell-mode"]
	path = site-lisp/haskell-mode
	url = https://github.com/haskell/haskell-mode.git
	branch = master
        ignore=dirty
[submodule "site-lisp/use-package"]
	path = site-lisp/use-package
	url = https://github.com/jwiegley/use-package.git
	branch = master
[submodule "site-lisp/org-mode"]
	path = site-lisp/org-mode
	url = git://orgmode.org/org-mode.git
	branch = master
        ignore=dirty
[submodule "site-lisp/proofgeneral"]
	path = site-lisp/proofgeneral
